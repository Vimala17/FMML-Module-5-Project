    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "from sklearn.feature_extraction.text import CountVectorizer, TfidfVectorizer, HashingVectorizer\n",
        "from sklearn.cluster import KMeans\n",
        "from sklearn.metrics import silhouette_score\n",
        "\n",
        "# Sample text data\n",
        "documents = [\n",
        "    \"Machine learning is amazing\",\n",
        "    \"I love machine learning\",\n",
        "    \"Deep learning is a branch of machine learning\",\n",
        "    \"AI and machine learning are transforming industries\",\n",
        "    \"Machine learning enables intelligent systems\",\n",
        "]\n",
        "\n",
        "# Define different BoW vectorizers\n",
        "vectorizers = {\n",
        "    \"Count Vectorizer\": CountVectorizer(),\n",
        "    \"Binary BoW\": CountVectorizer(binary=True),\n",
        "    \"TF-IDF\": TfidfVectorizer(),\n",
        "    \"Bigrams (2-grams)\": CountVectorizer(ngram_range=(2, 2)),\n",
        "    \"Trigrams (3-grams)\": CountVectorizer(ngram_range=(3, 3)),\n",
        "    \"Hashing (Fixed 10 Features)\": HashingVectorizer(n_features=10)\n",
        "}\n",
        "\n",
        "# Store results\n",
        "vectorized_data = {}\n",
        "\n",
        "for name, vectorizer in vectorizers.items():\n",
        "    matrix = vectorizer.fit_transform(documents)\n",
        "    vectorized_data[name] = matrix.toarray()\n",
        "\n",
        "# Convert to DataFrames for visualization\n",
        "dfs = {name: pd.DataFrame(data, columns=vectorizer.get_feature_names_out(), index=[f\"Doc {i+1}\" for i in range(len(documents))])\n",
        "       if hasattr(vectorizer, \"get_feature_names_out\") else pd.DataFrame(data)\n",
        "       for name, data in vectorized_data.items()}\n",
        "\n",
        "# Plot feature importance\n",
        "plt.figure(figsize=(12, 6))\n",
        "for i, (name, df) in enumerate(dfs.items()):\n",
        "    plt.subplot(2, 3, i+1)\n",
        "    sns.heatmap(df.T, cmap=\"Blues\", annot=True, fmt=\".2f\")\n",
        "    plt.title(name)\n",
        "    plt.xlabel(\"Documents\")\n",
        "    plt.ylabel(\"Features\")\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# K-Means Clustering & Silhouette Score Evaluation\n",
        "print(\"\\nSilhouette Scores for Different Vectorizations:\")\n",
        "for name, matrix in vectorized_data.items():\n",
        "    if matrix.shape[1] > 1:  # Ensure valid clustering dimensions\n",
        "        kmeans = KMeans(n_clusters=2, random_state=42, n_init=10)\n",
        "        cluster_labels = kmeans.fit_predict(matrix)\n",
        "        score = silhouette_score(matrix, cluster_labels)\n",
        "        print(f\"{name}: {score:.4f}\")"
      ],
